\n      {active && <div className={'scanner'} ref={ref}></div>}\r\n      {children}\r\n    </ScannerContext.Provider>\r\n  )\r\n}\r\n\r\nexport const useScanner = () => {\r\n  return useContext(ScannerContext);\r\n}\r\n","C:\\Users\\Peter\\Documents\\Capstone\\Pace-MS-Capstone\\frontend\\src\\components\\trackingLookup.js",["103"],[],"import React from 'react';\r\n\r\nexport default function FindTrackingStatus() {\r\n  // state variable to store the input string\r\n  const [input, setInput] = React.useState('');\r\n\r\n\r\n  //function to use RegEx to find carrier and lookup status\r\n    function findStatus(){\r\n    const trackingNo = input;\r\n    const regexUPS = /^1Z/;\r\n    const matchUPS = regexUPS.exec(trackingNo);\r\n\r\n    if (matchUPS==true){\r\n        const status = lookupUpsTrackingNumber(trackingNo);\r\n        alert(`The shipping status is: ${status}.`);\r\n        return status;\r\n    }\r\n    else\r\n        alert('the shipping status could not be found');\r\n\r\n    \r\n    function lookupUpsTrackingNumber(trackingNo) {\r\n        // Set the URL for the UPS tracking API\r\n        const trackingApiUrl = 'https://www.ups.com/track/api/Track/GetStatus?loc=en_US';\r\n      \r\n        // Send the request to UPS API\r\n        const requestOptions = {\r\n          method: 'POST',\r\n          headers: {\r\n            'Content-Type': 'application/json'\r\n          },\r\n          body: JSON.stringify({\r\n            Locale: 'en_US',\r\n            Requester: 'UPSHome',\r\n            TrackingNumber: [trackingNo]\r\n          })\r\n        };\r\n      \r\n        // Make a request to the UPS tracking API to get the tracking information\r\n        const response = fetch(trackingApiUrl, requestOptions);\r\n      \r\n        // Process the response to extract the tracking information\r\n        const trackingInfo = response.trackingInfo;\r\n      \r\n        // Return the tracking information\r\n        return trackingInfo;\r\n\r\n    }\r\n\r\n  }\r\n\r\n  return (\r\n    <div>\r\n      <input\r\n        type=\"text\"\r\n        value={input}\r\n        onChange={event => setInput(event.target.value)}\r\n      />\r\n      <button onClick={findStatus}>Lookup Shipment</button>\r\n    </div>\r\n  );\r\n}\r\n",{"ruleId":"104","severity":1,"message":"105","line":1,"column":8,"nodeType":"106","messageId":"107","endLine":1,"endColumn":12},{"ruleId":"104","severity":1,"message":"108","line":2,"column":17,"nodeType":"106","messageId":"107","endLine":2,"endColumn":25},{"ruleId":"104","severity":1,"message":"109","line":5,"column":10,"nodeType":"106","messageId":"107","endLine":5,"endColumn":16},{"ruleId":"104","severity":1,"message":"110","line":5,"column":18,"nodeType":"106","messageId":"107","endLine":5,"endColumn":25},{"ruleId":"111","severity":1,"message":"112","line":11,"column":17,"nodeType":"113","endLine":11,"endColumn":49},{"ruleId":"111","severity":1,"message":"112","line":17,"column":17,"nodeType":"113","endLine":17,"endColumn":74},{"ruleId":"111","severity":1,"message":"112","line":43,"column":13,"nodeType":"113","endLine":43,"endColumn":71},{"ruleId":"111","severity":1,"message":"112","line":44,"column":13,"nodeType":"113","endLine":44,"endColumn":45},{"ruleId":"111","severity":1,"message":"112","line":51,"column":9,"nodeType":"113","endLine":51,"endColumn":64},{"ruleId":"111","severity":1,"message":"112","line":56,"column":21,"nodeType":"113","endLine":56,"endColumn":97},{"ruleId":"114","severity":1,"message":"115","line":56,"column":65,"nodeType":"116","messageId":"117","endLine":56,"endColumn":96},{"ruleId":"111","severity":1,"message":"112","line":58,"column":21,"nodeType":"113","endLine":58,"endColumn":84},{"ruleId":"111","severity":1,"message":"112","line":60,"column":29,"nodeType":"113","endLine":60,"endColumn":67},{"ruleId":"111","severity":1,"message":"112","line":61,"column":29,"nodeType":"113","endLine":61,"endColumn":67},{"ruleId":"111","severity":1,"message":"112","line":62,"column":29,"nodeType